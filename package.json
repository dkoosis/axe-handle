{
  "name": "axe-handle",
  "version": "0.1.0",
  "description": "MCP Server Generator for Express.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "axe-handle": "dist/cli.js"
  },
  "scripts": {
    "refactor": "ts-node refactoring-script.ts",
    "build": "tsc",
    "postbuild": "cp -r templates dist/",
    "start": "node dist/index.js",
    "dev": "ts-node -r module-alias/register src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "generate": "ts-node -r module-alias/register src/cli.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "generator",
    "express",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "@types/module-alias": "^2.0.4",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "eta": "^2.2.0",
    "express": "^4.18.2",
    "glob": "^10.4.5",
    "module-alias": "^2.2.3",
    "neverthrow": "^6.2.0",
    "protobufjs": "^7.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.56.0",
    "eslint-plugin-neverthrow": "^1.1.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "@types/module-alias": "^2.0.4"
  },
  "_moduleAliases": {
    "@axe": "dist/axe",
    "@generators": "dist/generators",
    "@utils": "dist/utils",
    "@templates": "templates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/axe-handle.git"
  },
  "engines": {
    "node": ">=18"
  }
}