{
  "name": "axe-handle",
  "version": "0.1.0",
  "description": "MCP Server Generator for Express.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "axe-handle": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate": "ts-node src/cli.ts",
    "start:generated": "node generated/server.js",
    "dev": "concurrently \"npm:watch\" \"npm:test:watch\"",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "generator",
    "express",
    "typescript"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.22",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "eta": "^2.2.0",
    "express": "^4.18.2",
    "protobufjs": "^7.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkoosis/axe-handle.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/dkoosis/axe-handle/issues"
  },
  "homepage": "https://github.com/dkoosis/axe-handle#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  }
}
